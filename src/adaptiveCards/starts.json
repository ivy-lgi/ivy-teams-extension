{
  "$schema": "http://adaptivecards.io/schemas/adaptive-card.json",
  "type": "AdaptiveCard",
  "version": "1.4",
  "body": [
    {
      "type": "TextBlock",
      "text": "Starts",
      "size": "Large",
      "weight": "Bolder",
      "wrap": true
    },
    {
      "type": "Container",
      "items": [
        {
          "$data": "${starts}",
          "type": "ActionSet",
          "actions": [
            {
              "type": "Action.OpenUrl",
              "title": "${name}",
              "url": "${fullRequestPath}"
            }
          ]
        }
      ]
    }
  ]
}
